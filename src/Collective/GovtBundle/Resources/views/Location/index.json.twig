{
    "type": "FeatureCollection",
    "features": [
{% for d in data %}    
    { 
        "type": "Feature",
        "geometry": { 
            "type": "Point", 
            "coordinates": [{{ d.lon }}, {{ d.lat }}]
        }, 
        "properties": {
            "name" : "{{ d.name }}"
        }
    }{% if not loop.last %},{% endif %}{% endfor %}]
}