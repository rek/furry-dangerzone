{
    "type"  : "Location",
    "title" : "{{ location }}",
    "links" : [
    {% for l in location.links %}     
    { 
        "id"   : "{{ l.id }}",
        "type" : "{{ l.type }}",
        "title": "{{ l.title }}",
        "link" : "{{ l.link }}"
    }{% if not loop.last %},{% endif %}{% endfor %}]
}